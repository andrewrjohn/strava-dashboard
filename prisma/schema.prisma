generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id                   String @id @default(uuid())
  strava_athlete_id    Int    @unique
  strava_access_token  String @unique
  strava_refresh_token String @unique
  strava_expires_at    Int
}
